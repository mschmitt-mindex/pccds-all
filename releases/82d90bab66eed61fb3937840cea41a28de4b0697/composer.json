{
  "name": "icir/icir-prime-care",
  "description": "Craft CMS",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "project"
  ],
  "license": "MIT",
  "homepage": "https://craftcms.com/",
  "type": "project",
  "support": {
    "email": "support@craftcms.com",
    "issues": "https://github.com/craftcms/cms/issues",
    "forum": "https://craftcms.stackexchange.com/",
    "source": "https://github.com/craftcms/cms",
    "docs": "https://craftcms.com/docs",
    "rss": "https://craftcms.com/changelog.rss"
  },
  "require": {
    "php": ">=7.0.0",
    "craftcms/cms": "3.4.15",
    "vlucas/phpdotenv": "v2.4.0",
    "nystudio107/craft-scripts": "1.2.7",
    "craftcms/redactor": "2.6.0.1",
    "mmikkel/cp-field-inspect": "1.1.2",
    "nystudio107/craft-fastcgicachebust": "1.0.7",
    "nystudio107/craft-minify": "1.2.9",
    "nystudio107/craft-typogrify": "1.1.18",
    "nystudio107/craft-seomatic": "3.2.43",
    "verbb/image-resizer": "2.0.6",
    "wbrowar/adminbar": "3.1.8.1",
    "wbrowar/contentstats": "2.1.1",
    "wbrowar/guide": "2.1.3.3",
    "aelvan/imager": "v2.4.0",
    "supercool/tablemaker": "2.0.1",
    "craftcms/feed-me": "4.2.0.1",
    "solspace/craft-freeform": "3.6.2",
    "verbb/super-table": "2.4.4"
  },
  "autoload": {
    "psr-4": {
      "modules\\": ["modules/csvescapermodule/src/","modules/cameltokebabmodule/src/"]
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@composer dump-autoload -o",
      "@php craft setup/welcome"
    ],
    "post-update-cmd": [
      "if [ -f ./scripts/.env.sh ] && [ ! -L ./vendor/nystudio107/craft-scripts/scripts/.env.sh ]; then cp -fp ./scripts/.env.sh ./vendor/nystudio107/craft-scripts/scripts/.env.sh; fi",
      "if [ -f ./vendor/nystudio107/craft-scripts/scripts/.env.sh ]; then ./vendor/nystudio107/craft-scripts/scripts/clear_caches.sh; fi"
    ],
    "post-install-cmd": [
      "if [ -f ./scripts/.env.sh ] && [ ! -L ./vendor/nystudio107/craft-scripts/scripts/.env.sh ]; then cp -fp ./scripts/.env.sh ./vendor/nystudio107/craft-scripts/scripts/.env.sh; fi",
      "if [ -f ./vendor/nystudio107/craft-scripts/scripts/.env.sh ]; then ./vendor/nystudio107/craft-scripts/scripts/clear_caches.sh; fi"
    ]
  }
}
